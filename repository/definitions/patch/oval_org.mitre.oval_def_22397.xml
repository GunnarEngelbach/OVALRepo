    <oval-def:title>ELSA-2008:0159: dbus security update (Moderate)</oval-def:title>
    <oval-def:affected family="unix">
      <oval-def:platform>Oracle Linux 5</oval-def:platform>
      <oval-def:product>dbus</oval-def:product>
