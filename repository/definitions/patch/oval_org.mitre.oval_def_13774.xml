    <oval-def:title>DSA-1716-1 vnc4 -- integer overflow</oval-def:title>
    <oval-def:affected family="unix">
      <oval-def:platform>Debian GNU/Linux 4.0</oval-def:platform>
      <oval-def:product>vnc4</oval-def:product>
