    <oval-def:title>DSA-1959-1 ganeti -- missing input sanitation</oval-def:title>
    <oval-def:affected family="unix">
      <oval-def:platform>Debian GNU/Linux 5.0</oval-def:platform>
      <oval-def:product>ganeti</oval-def:product>
